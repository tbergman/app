{
  "private": true,
  "name": "hedvig-app",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src hedvig-redux App.js index.js --max-warnings 0",
    "postinstall": "rndebugger-open;react-native-schemes-manager all && yarn run create-podspecs",
    "create-podspecs": "podspec-generator node_modules/react-native-analytics-segment-io --ios=9.0 --source_files=ios/RNAnalyticsSegmentIO --dependencies=React,Analytics",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "postversion": "react-native-version",
    "generate-graphql-types": "ts-node scripts/generate-graphql-types.js",
    "generate-graphql-components": "CODEGEN_NO_NAMESPACES=true gql-gen --schema https://graphql.dev.hedvigit.com/graphql --template graphql-codegen-typescript-react-apollo-template --out ./src/graphql/components.tsx './src/**/*.graphql'"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "./src/setupTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
